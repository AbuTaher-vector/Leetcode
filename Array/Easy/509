class Solution {
public:
    int make(int i)
    {
        if(i==0) return 0;
        if(i==1) return 1;
        return make(i-1)+make(i-2);
        
     }
    int fib(int n) {
        int ans=make(n);
        return ans;
        
    }
};

// Link : https://leetcode.com/problems/fibonacci-number/description/
